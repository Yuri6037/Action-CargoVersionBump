{
  "name": "cargo-version-bump",
  "version": "0.0.0",
  "private": true,
  "description": "TypeScript template action",
  "main": "lib/src/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Yuri6037/Action-CargoVersionBump.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.6.0",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^5.0.3",
    "@octokit/core": "^4.0.5",
    "@octokit/webhooks-types": "^6.3.6",
    "async-line-reader": "^1.0.0",
    "axios": "^0.27.2",
    "line-replace": "^2.0.1",
    "semver": "^7.3.7"
  },
  "devDependencies": {
    "@types/node": "^18.7.21",
    "@types/semver": "^7.3.12",
    "@vercel/ncc": "^0.34.0",
    "eslint": "^8.23.0",
    "eslint-plugin-github": "^4.3.7",
    "eslint-plugin-jest": "^27.0.2",
    "jest": "^27.2.5",
    "js-yaml": "^4.1.0",
    "prettier": "2.7.1",
    "ts-jest": "^27.1.2",
    "typescript": "^4.8.3"
  }
}